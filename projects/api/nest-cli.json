{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "api/src/main",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": [
            "dto",
            "entity",
            "model"
          ]
        }
      }
    ]
  }
}